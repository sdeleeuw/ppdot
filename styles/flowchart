//- Base:

//! style base | shape    | box        | n
//! style base | style    | "filled"   | n
//! style base | penwidth | 1.5        | ne
//! style base | fontname | m:fontname | ne
//! style base | fontsize | m:fontsize | ne

//- Colors:

//! style red    | color     | "m:red3"             | ne
//! style red    | fillcolor | "m:red2:m:red"       | n
//! style yellow | color     | "m:yellow3"          | ne
//! style yellow | fillcolor | "m:yellow2:m:yellow" | n
//! style blue   | color     | "m:blue3"            | ne
//! style blue   | fillcolor | "m:blue2:m:blue"     | n
//! style orange | color     | "m:orange3"          | ne
//! style orange | fillcolor | "m:orange2:m:orange" | n
//! style green  | color     | "m:green3"           | ne
//! style green  | fillcolor | "m:green2:m:green"   | n
//! style violet | color     | "m:violet3"          | ne
//! style violet | fillcolor | "m:violet2:m:violet" | n

//- Graph:

//! style flowchart | label     | m:title    | g
//! style flowchart | labelloc  | t          | g
//! style flowchart | labeljust | r          | g
//! style flowchart | fontname  | m:fontname | g
//! style flowchart | fontsize  | m:fontsize | g
//! style flowchart | bgcolor   | m:bgcolor  | g

//- Nodes:

//! copy base     | terminal | n
//! copy red      | terminal | n
//! copy terminal | start    | n
//! copy terminal | end      | n
//! copy green    | end      | n
//! copy base     | process  | n
//! copy blue     | process  | n
//! copy base     | decision | n
//! copy yellow   | decision | n
//! copy base     | inout    | n
//! copy violet   | inout    | n

//! style start    | shape | ellipse          | n
//! style end      | shape | ellipse          | n
//! style decision | shape | diamond          | n
//! style inout    | shape | parallelogram    | n
//! style terminal | style | "filled,rounded" | n
//! style decision | style | "filled,rounded" | n

